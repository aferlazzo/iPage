#Maze {top:0;left:0;z-index:95;width:100%;height:480px;
visibility:hidden;background: url("./images/MazeGreen.gif") no-repeat;}
#a { white-space:nowrap; }
ol li{ padding-bottom:4px;margin:0 6em 0 4em; }
ol p{ text-align:justify; }
#Box2 {float:left;width:9em;margin-right:2px;visibility:visible;}
#EndPosition {
float:left;
height:120px;
width:120px;
padding-top:12px;
padding-left:66px;
visibility:visible;
text-align:center;
background: url("./images/Bolt/LoanApproval.gif") no-repeat center;
}
#Box3 {float:left;width:9em;white-space:nowrap;visibility:visible;}
#Box1a, #Box2a, #Box3a {float:left;width:208px;margin-right:15px;vertical-align:top;}
#Box2 a, #Box3 a {color:#fff;font-size:50%;text-decoration:none;}
#Box2 ul, #Box3 ul {margin-left:.5em;margin-top:0;}
#Box2 a:hover, #Box3 a:hover {color:#060;background:#fff;}
#Lucio {
position:absolute;
z-index:40;
left:0;
top:310px;
width:150px;
height:120px;
visibility:hidden;
background-image: url("images/Bolt/Bolt.gif");
}
#BoxesOut {
z-index:50;
position:absolute;
top:275px;
height:142px;
width:100%;
text-align:middle;
}
#BoxesIn {
visibility:visible;
z-index:50;
position:relative;
height:140px;
width:665px;
margin:0 auto;
text-align:middle;
}
