/* begin styles for RSS Feed */
.rss_box {
 margin: 10px 1em 0 1em;
 padding: 4px 8px;
 width: 120px;
 background-color:transparent; /*#ededed;*/
 /*border: 2px dashed #009;*/
 border-left:1px solid #009;
 z-index:5;
}

.rss_title, rss_title a {
font-family: "American Typewriter", "Trebuchet MS", Trebuchet, Lucida, sans-serif;
font-size:medium;
text-align:left;
font-weight:bold;
margin: 5px 0;
padding: 0;
letter-spacing: 1px;
}

.rss_items {
  
}

.rss_item  {
  font-family: verdana, arial, sans-serif;
  font-size:x-small;
  font-weight : bold; 
  margin: 8px 0;
}

.rss_item a:link, .rss_item a:visited, .rss_item a:active {
text-decoration : none;
border-bottom: 1px solid #ededed;
color: #ac025b; /*color: #88b;*/
}

.rss_item a:hover { 
text-decoration : none;
color: #ac025b;/*color: #e0861e;*/
border-bottom: 1px dotted #e0861e;
}

.rss_date {
font-size:x-small;
font-weight : normal;
color: #F60;
}
TD.DT{
padding:5px 0px 5px 0px;
vertical-align:top;
text-align:right;
font-weight:bold;
color:#009;
}
TD.DL{
padding:5px 0px 5px 0px;
vertical-align:top;
text-align:left;
font-weight:bold;
color:#009;
}
TD.DD{
padding:5px 5px 5px 5px;
vertical-align:top;
text-align:left;
color:#009;
}
a.DT{
font-weight:bold;
color:#009;
text-decoration:none;
border-bottom:1px solid #00dceb;
}
a.DD{
font-weight:bold;
color:#009;
text-decoration:none;
border-bottom:1px solid #00dceb;
}
a.DD:visited,a.DT:visited{border-bottom:1px dashed #00aaeb;}
a.DT:hover,a.DD:hover,a:hover {background:#039;}
.Box {position:relative;border:2px dotted #f00;padding:0 .75em .75em;text-align:left;z-index:5;}
.Box p,.Box p:first-letter, .Box ol, .Box ul {font-size:medium;color:#039}
/*.Box p { padding:0 }*/
.BoxHeading { 
z-index:5;
position: relative; 
top: -20px;
left:1em; 
border:2px solid #fff;
font-size:24px;
background:#f00;
color:#fff;
padding:0 4px;
margin-bottom:-1em;}
/*pullquote stuff*/
.PullQuote{
width: 22%;
float:right;
font:115% normal Verdana,Arial,Helvetica,sans-serif;
color:#009;
margin:5px;
padding:1em;
text-align:left;
background-color:transparent;
background-image:url(../images/closequote.gif);
background-repeat: no-repeat;
background-position: bottom right;
border:3px #009 solid;
padding:10px .5em 10px .5em;
}
.PullQuote:first-letter { 
font:100% normal Verdana,Arial,Helvetica,sans-serif;
background-image:url(../images/openquote.gif);
background-repeat:no-repeat;
background-position:0 0;
padding:0 0 5px 2em;
}

.Highlight {background:#ff0;}
.bl {background: url(../images/bl-blue.gif) 0 100% no-repeat #009;width:16em;font-size:24px;}
.br {background: url(../images/br-blue.gif) 100% 100% no-repeat;}
.tl {background: url(../images/tl-blue.gif) 0 0 no-repeat;}
.tr {background: url(../images/tr-blue.gif) 100% 0 no-repeat; padding:10px;color:#fff;font-size:24px;}
.clear {font-size: 1px; height: 1px;}
/*---rounded corners ---*/
#container {width:12em;margin:0 auto;}

dl {margin:0;padding:0;border-left:3px solid #009;border-right:3px solid #009;}
dt {margin:0;padding:0;text-align:center;font-size:1.4em;font-weight:bold;color:#fff;background:#039;}
dd {margin:0 0 -2px;padding:0; text-align:center;border-top:0;}
#gallery a, #gallery a:visited {color:#fff;text-decoration:none;display:block;padding:0.4em 1em;background:#039;}

#gallery a:hover {background:#fff;color:#009;}

.top {
  display:block; 
  background:transparent; 
  font-size:1px;
  }

.bottom {
  display:block; 
  background:transparent; 
  font-size:1px; 
  /*border-top:1px solid #fff;*/
  }
.b1, .b2, .b3, .b4 {
display:block; 
/* hide overflow:hidden from IE5/Mac */ 
/* \*/ 
overflow: hidden; 
/* */ 
}
.b1, .b2, .b3 {height:1px;}
.b2, .b3, .b4 {background:#039;border-left:1px solid #f0ffff; border-right:1px solid #f0ffff;}
.b1 {margin:0 5px; background:#f0ffff;}
.b2 {margin:0 3px; border-width:0 2px;}
.b3 {margin:0 2px;}
.b4 {height:2px; margin:0 1px;}
h2{
text-align:center;
font-weight:bold;
font-size:medium;
color:#fff;
border:none;width:99%;background:#039;margin:0;padding:0;border-left:1px solid #f0ffff;border-right:1px solid #f0ffff;}
