/* begin styles for RSS Feed */
.rss_box {
 margin: 10px 1em 0 1em;
 padding: 4px 8px;
 width: 120px;
 background-color:transparent; /*#ededed;*/
 /*border: 2px dashed #906;*/
 border-left:1px solid #906;
 z-index:5;
}

.rss_title, rss_title a {
font-family: "American Typewriter", "Trebuchet MS", Trebuchet, Lucida, sans-serif;
font-size:medium;
text-align:left;
font-weight:bold;
margin: 5px 0;
padding: 0;
letter-spacing: 1px;
}

.rss_items {
  
}

.rss_item  {
  font-family: verdana, arial, sans-serif;
  font-size:x-small;
  font-weight : bold; 
  margin: 8px 0;
}

.rss_item a:link, .rss_item a:visited, .rss_item a:active {
text-decoration : none;
border-bottom: 1px solid #ededed;
color: #906; /*color: #88b;*/
}

.rss_item a:hover { 
text-decoration : none;
color: #fff; /* 7/24 was #906;*/
border-bottom: 1px dotted #e0861e;
}

.rss_date {
font-size:x-small;
font-weight : normal;
color: #F60;
}
TD.DT{
padding:5px 0px 5px 0px;
vertical-align:top;
text-align:right;
font-weight:bold;
color:#906;
}
TD.DL{
padding:5px 0px 5px 0px;
vertical-align:top;
text-align:left;
font-weight:bold;
color:#906;
}
TD.DD{
padding:5px 5px 5px 5px;
vertical-align:top;
text-align:left;
color:#906;
}
a.DT{
font-weight:bold;
color:#906;
text-decoration:none;
border-bottom:1px solid #00dceb;
}
a.DD{
font-weight:bold;
color:#906;
text-decoration:none;
border-bottom:1px solid #00dceb;
}
a.DD:visited,a.DT:visited{border-bottom:1px dashed #00aaeb;}
a.DT:hover,a.DD:hover,a:hover {background:#906;}
/*pullquote stuff*/
.PullQuote {
width: 22%;
float:right;
font:115% normal Verdana,Arial,Helvetica,sans-serif;
color:#009;
margin:0 15px;
padding:1em;
text-align:left;
background-color:transparent;
background-image:url(../images/closequote.gif);
background-repeat: no-repeat;
background-position: bottom right;
border:3px #906 solid;
padding:10px .5em 10px .5em;
}
.PullQuote:first-letter { 
font:100% normal Verdana,Arial,Helvetica,sans-serif;
background-image:url(../images/openquote.gif);
background-repeat:no-repeat;
background-position:0 0;
padding:0 0 5px 2em;
}
.Highlight {background:#ff0;}
/*---rounded corners ---*/
#container {width:12em;margin:0 auto;}

dl {margin:0;padding:0;border-left:3px solid #906;border-right:3px solid #906;}
dt {margin:0;padding:0;text-align:center;font-size:1.4em;font-weight:bold;color:#fff;background:#906;}
dd {margin:0 0 -2px;padding:0; text-align:center;border-top:0;}
#gallery a, #gallery a:visited {color:#fff;text-decoration:none;display:block;padding:0.4em 1em;background:#906;}

#gallery a:hover {background:#fff;color:#906;}

.top {
  display:block; 
  background:transparent; 
  font-size:1px;
  }

.bottom {
  display:block; 
  background:transparent; 
  font-size:1px; 
  /*border-top:1px solid #fff;*/
  }
.b1, .b2, .b3, .b4 {
display:block; 
/* hide overflow:hidden from IE5/Mac */ 
/* \*/ 
overflow: hidden; 
/* */ 
}
.b1, .b2, .b3 {height:1px;}
.b2, .b3, .b4 {background:#906;border-left:1px solid #fff0f5; border-right:1px solid #fff0f5;}
.b1 {margin:0 5px; background:#fff0f5;}
.b2 {margin:0 3px; border-width:0 2px;}
.b3 {margin:0 2px;}
.b4 {height:2px; margin:0 1px;}
h2{
text-align:center;
font-weight:bold;
font-size:medium;
color:#fff;
border:none;width:99%;background:#906;margin:0;padding:0;border-left:1px solid #fff0f5;border-right:1px solid #fff0f5;}
h2.NextSteps{
width:7em;
margin:0;
}
/*---end rounded corners---*/

