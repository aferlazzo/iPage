/* begin styles for RSS Feed */
.rss_box {
margin: 10px .5em 0 0;
padding: 4px .5em;
width: 120px;
border-left:1px solid #976;
z-index:5;
}
.rss_title, rss_title a {
font-family: "American Typewriter", "Trebuchet MS", Trebuchet, Lucida, sans-serif;
font-size:medium;
text-align:left;
font-weight:bold;
margin: 5px 0;
padding: 0;
letter-spacing: 1px;
}
.rss_items {
}
.rss_item  {
font-family: verdana, arial, sans-serif;
font-size:x-small;
font-weight : bold; 
margin: 8px 0;
}
.rss_item a:link, .rss_item a:visited, .rss_item a:active {
text-decoration : none;
border-bottom: 1px solid #ededed;
color: #663; /*#ac025b; */
}
.rss_item a:hover { 
text-decoration : none;
color: #fff; /*#ac025b;#e0861e;*/
border-bottom: 1px dotted #e0861e;
}
.rss_date {
font-size:x-small;
font-weight : normal;
color: #F60;
}
div.QuickHits {
 margin: 0 0 10px 10px;
 padding: 4px 2px;
 width: 120px;
 float:left;
 z-index:5;
}
/*pullquote stuff*/
.PullQuote {
width: 24%;
float:right;
font:115% normal Verdana,Arial,Helvetica,sans-serif;
color:#009;
margin:8px;

text-align:left;
background-color:transparent;
background-image:url("../images/sprites2.png") 0 -396px;}
border: 3px #976 solid;
padding: 10px 5px 10px  25px;
}
.PullQuote:first-letter { 
font:100% normal Verdana,Arial,Helvetica,sans-serif;
background-image:url("../images/sprites2.png") 0 -432px;}
/*padding:0 0 5px 2em;*/
}
.Highlight { background:#ff0; }
/*---rounded corners ---*/
#container {width:12em;margin:0 auto;}

dl {margin:0;padding:0;border-left:3px solid #976;border-right:3px solid #976;}
dt {margin:0;padding:0;text-align:center;font-size:1.4em;font-weight:bold;color:#fff;background:#976;}
dd {margin:0 0 -2px;padding:0; text-align:center;border-top:0;}
#gallery a, #gallery a:visited {color:#fff;text-decoration:none;display:block;padding:0.4em 1em;background:#976;}

#gallery a:hover {background:#fff;color:#976;}

.top {
  display:block; 
  background:transparent; 
  font-size:1px;
  }

.bottom {
  display:block; 
  background:transparent; 
  font-size:1px; 
  /*border-top:1px solid #fff;*/
  }
.b1, .b2, .b3, .b4 {
display:block; 
/* hide overflow:hidden from IE5/Mac */ 
/* \*/ 
overflow: hidden; 
/* */ 
}
.b1, .b2, .b3 {height:1px;}
.b2, .b3, .b4 {background:#976;border-left:1px solid #fff; border-right:1px solid #fff;}
.b1 {margin:0 5px; background:#fff;}
.b2 {margin:0 3px; border-width:0 2px;}
.b3 {margin:0 2px;}
.b4 {height:2px; margin:0 1px;}
h2{
text-align:center;
font-weight:bold;
font-size:medium;
color:#fff;
border:none;width:99%;background:#976;margin:0;padding:0;border-left:1px solid #eee9bf;border-right:1px solid #eee9bf;}
h2.NextSteps{
width:7em;
margin:0;
}
/*---end rounded corners---*/
