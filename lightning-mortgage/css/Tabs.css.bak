div#nav ul#nav-list,
div#navTab ul#nav-list {
	margin:0 auto;
	padding: 0;
	width:720px; /* enough room for all tabs */
	list-style-type: none;
}
ul#nav-list li,
div#nav ul#nav-list li,
div#navTab ul#nav-list li {
	float: left;
	margin:0;padding:0;
	width:120px;
	height:35px;
	overflow: hidden;
	background: url('../images/sprites.png') no-repeat;
	background-position: 0px -475px;
}
div#nav ul#nav-list li.prob,
div#navTab ul#nav-list li.prob {
	width:120px; /*was 130 */
	background: url('../images/sprites.png') no-repeat;
	background-position: 0px -475px;
}
div#nav ul#nav-list li a,
div#navTab ul#nav-list li a {
	display: block;
	margin:0;padding:0;
	width:120px;
	text-align: center;
	color: #009;
	font:bold 12px/40px "Trebuchet MS";
	letter-spacing: -0.05em;
	text-transform: uppercase;
	text-decoration:none;
	background:transparent;
}

div#nav ul#nav-list li.prob a,
div#navTab ul#nav-list li.prob a {
	width:120px; /* was 130 */
	margin:0;padding:0;
	text-decoration:none;
	background:transparent;
}
div#nav ul#nav-list li a:hover,
div#navTab ul#nav-list li a:hover {
	text-decoration: none;
	color:#ff0;
	background:transparent;
}
